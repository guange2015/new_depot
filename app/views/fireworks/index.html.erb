
<!-- <h1>仓库库存列表</h1> -->

<%= render partial:'firework_tree', locals: {categories: @categories} %>

<table class="table table-bordered table-hover">
	<tr>
	<th>品名</th>
	<th>规格</th>
	<th>库存</th>
	<th>换算率</th>
	<th>操作</th>
	</tr>

	<% @fireworks.each do |f| %>
	<tr>
		<td><%= link_to f.name, f %></td>
		<td><%= f.spec %></td>
		<td><%= f.lastdata %></td>
		<td><%= f.rate %></td>
		<td><%= link_delete f,f.data_lists.empty? %></td>
	</tr>
	<% end %>
</table>

<%= paginate @fireworks %>
<div class="clear" />

  
